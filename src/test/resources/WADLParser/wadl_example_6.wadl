            </resource>
            <resource path="weather/{state}">
                <param name="state" required="true" style="template" type="xsd:string"/>
            </resource>
        </request>
        <response Status="200">
            <representation element="WEATHERDATA.v1:WeatherInfo" id="WEATHERDATA.v1.xsd"
                            mediaType="text/xml"/>
        </response>
    </method>
