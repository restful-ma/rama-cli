            </resource>
            <resource path="weather/{state}">
                <param name="state" required="true" style="template" type="xsd:string"/>
            </resource>
            <representation element="QE_FLIGHTPLAN_SYNC.VERSION_1:QE_FLIGHTPLAN_SYNC"
